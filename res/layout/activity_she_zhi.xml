<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/bg"
    android:orientation="vertical"
    tools:context="com.example.com.jglx.android.app.ui.MySheZhiActivity" >

    <TextView
        android:id="@+id/s_LL_yonhufank"
        android:layout_width="match_parent"
        android:layout_height="@dimen/text_height"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/all_magrin"
        android:background="@drawable/bg_line_top_bottom"
        android:drawableRight="@drawable/my_imag_right"
        android:gravity="left|center_vertical"
        android:paddingLeft="@dimen/all_magrin"
        android:paddingRight="@dimen/all_magrin"
        android:text="用户反馈" />

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/all_magrin"
        android:background="@drawable/bg_line_top_bottom"
        android:orientation="vertical" >

        <LinearLayout
            android:id="@+id/s_LL_anquansezi"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/text_height"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/s_tv_anquansezi"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_gravity="center_horizontal"
                android:layout_weight="9"
                android:gravity="left|center_vertical"
                android:paddingLeft="@dimen/all_magrin"
                android:text="安全设置" />

            <TextView
                android:id="@+id/s_tv_anquansezi_right"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:drawableLeft="@drawable/s_tv_anquansezi_right"
                android:drawableRight="@drawable/my_imag_right"
                android:paddingRight="@dimen/all_magrin" />
        </LinearLayout>

        <View style="@style/divider_horizontal" />
        <!-- 清除缓存 -->

        <LinearLayout
            android:id="@+id/s_LL_qincufancun"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/text_height"
            android:background="@color/white"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/s_tv_qincufancun"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_gravity="center_horizontal"
                android:layout_weight="9"
                android:gravity="left|center_vertical"
                android:paddingLeft="@dimen/all_magrin"
                android:text="清除缓存" />

            <TextView
                android:id="@+id/s_tv_qincufancun_right"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:drawableRight="@drawable/my_imag_right"
                android:gravity="center"
                android:paddingRight="10dp"
                android:text="31.8MB" />
        </LinearLayout>

        <View style="@style/divider_horizontal" />

        <TextView
            android:id="@+id/s_LL_yonhuxiyi"
            android:layout_width="match_parent"
            android:layout_height="@dimen/text_height"
            android:drawableRight="@drawable/my_imag_right"
            android:gravity="left|center_vertical"
            android:paddingLeft="@dimen/all_magrin"
            android:paddingRight="@dimen/all_magrin"
            android:text="用户协议" />
    </LinearLayout>

    <TextView
        android:id="@+id/s_LL_guanyuwomen"
        android:layout_width="match_parent"
        android:layout_height="@dimen/text_height"
        android:layout_gravity="center_horizontal"
        android:layout_marginBottom="@dimen/all_magrin"
        android:layout_marginTop="@dimen/all_magrin"
        android:background="@drawable/bg_line_top_bottom"
        android:drawableRight="@drawable/my_imag_right"
        android:gravity="left|center_vertical"
        android:paddingLeft="@dimen/all_magrin"
        android:paddingRight="@dimen/all_magrin"
        android:text="关于我们" />

    <TextView
        android:id="@+id/s_LL_tuicun"
        android:layout_width="match_parent"
        android:layout_height="@dimen/text_height"
        android:background="@drawable/bg_line_top_bottom"
        android:gravity="center"
        android:text="退出" />

</LinearLayout>